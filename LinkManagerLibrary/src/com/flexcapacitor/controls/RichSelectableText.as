/**
 * Enables selection of text. 
 * Extends Rich Editable Text and turns off editing and tab focus. 
 * */
package com.flexcapacitor.controls {
	import spark.components.RichEditableText;

	public class RichSelectableText extends RichEditableText {
		
		public function RichSelectableText() {
			super();
			editable = false;
			selectable = true;
			tabFocusEnabled = false;
		}
	}
}